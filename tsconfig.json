{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "moduleResolution": "Node",
    "outFile": "dist/index.js",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "ES6", "DOM.Iterable"]
  },
  "exclude": [
    "./dist"
  ]
}
